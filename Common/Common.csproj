<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
	<Title>SphyrnidaeTech Common Code</Title>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <AssemblyName>Sphyrnidae.Common</AssemblyName>
    <RootNamespace>Sphyrnidae.Common</RootNamespace>
    <Version>2.0.0</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Doug Bartels</Authors>
    <Company>SphyrnidaeTech</Company>
    <Product>SphyrnidaeTech</Product>
    <Description>Sphyrnidae Common/Shared Code</Description>
    <Copyright>Â©2021 SphyrnidaeTech</Copyright>
    <PackageTags>Sphyrnidae SphyrnidaeTech Common Shared</PackageTags>
    <PackageReleaseNotes>Initial Creation against .net Core</PackageReleaseNotes>
    <IsPackable>true</IsPackable>
    <LangVersion>8</LangVersion>
    <OutputType>Library</OutputType>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseFile>SphyrnidaeTechLicense.txt</PackageLicenseFile>
    <RepositoryUrl>https://github.com/dbartels13/Common</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <PackageProjectUrl>https://dbartels13.github.io/Common/</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Dapper" Version="2.0.78" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc.NewtonsoftJson" Version="3.1.11" />
    <PackageReference Include="Microsoft.AspNetCore.SignalR.Client" Version="3.1.11" />
    <PackageReference Include="Microsoft.Extensions.Caching.StackExchangeRedis" Version="3.1.11" />
    <PackageReference Include="MySql.Data" Version="8.0.23" />
    <PackageReference Include="Swashbuckle.AspNetCore" Version="5.6.3" />
    <PackageReference Include="System.Data.SqlClient" Version="4.8.2" />
  </ItemGroup>

  <ItemGroup>
    <None Include="SphyrnidaeTechLicense.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
